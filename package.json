{
  "name": "combat",
  "version": "1.0.0",
  "description": "A real time, multi user battle combat for use in D&D",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "d": "^1.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "install": "^0.13.0",
    "mongoose": "^5.10.2",
    "npm": "^6.14.8",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-textarea-autosize": "^8.2.0",
    "save-dev": "0.0.1-security",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brian-Fairbanks/combat.git"
  },
  "keywords": [
    "d&d",
    "combat",
    "battle"
  ],
  "author": "Brian Fairbanks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Brian-Fairbanks/combat/issues"
  },
  "homepage": "https://github.com/Brian-Fairbanks/combat#readme"
}
